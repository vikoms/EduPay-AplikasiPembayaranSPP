<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.latihanujikompaket2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_edupay"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_edupay"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.detailpembayaran.DetailPembayaranActivity"
            android:theme="@style/dialogStyle" />
        <activity android:name=".ui.mainsiswa.MainSiswaActivity" />
        <activity android:name=".ui.datasiswa.DetailSiswaActivity" />
        <activity android:name=".ui.mainpetugas.MainPetugasActivity" />
        <activity
            android:name=".ui.datasiswa.ManageDataSiswaActivity"
            android:theme="@style/ActionBar" />
        <activity
            android:name=".ui.datapetugas.ManageDataPetugasActivity"
            android:theme="@style/dialogStyle" />
        <activity
            android:name=".ui.dataspp.ManageDataSppActivity"
            android:theme="@style/dialogStyle" />
        <activity
            android:name=".ui.datakelas.ManageDataKelasActivity"
            android:theme="@style/dialogStyle" />
        <activity
            android:name=".ui.datapetugas.DataPetugasActivity"
            android:theme="@style/ActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.admin.MainAdminActivity" />
        </activity>
        <activity
            android:name=".ui.dataspp.DataSppActivity"
            android:theme="@style/ActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.admin.MainAdminActivity" />
        </activity>
        <activity
            android:name=".ui.datakelas.DataKelasActivity"
            android:theme="@style/ActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.admin.MainAdminActivity" />
        </activity>
        <activity
            android:name=".ui.datasiswa.DataSiswaActivity"
            android:theme="@style/ActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.admin.MainAdminActivity" />
        </activity>
        <activity
            android:name=".ui.admin.MainAdminActivity"
            android:label="@string/title_activity_admin" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>